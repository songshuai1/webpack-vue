{
  "plugins": [
    "@babel/plugin-syntax-dynamic-import"
    // "@babel/plugin-proposal-optional-chaining"
  ],
  "presets": [
    [
      "@vue/babel-preset-jsx",
      {
        "compositionAPI": true
      }
    ],
    [
      "@babel/env",
      {
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "ie >= 10"
          ]
        },
        "modules": false
      }
    ]

  ]
}
